on boot
    chown system system /sys/devices/virtual/touch/tp_dev/fod_status

    chmod 0664 /sys/devices/virtual/touch/tp_dev/fod_status

service fingerprint-inscreen-1-0 /system/bin/hw/lineage.biometrics.fingerprint.inscreen@1.0-service.raphael
    interface vendor.lineage.biometrics.fingerprint.inscreen@1.0::IFingerprintInscreen default
    class hal
    user system
    group system wakelock
    shutdown critical
